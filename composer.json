        "yiisoft/yii2-httpclient": "^2.0",
        "deepziyu/yii-fast-api": "^0.2.1",
        "mdmsoft/yii2-admin": "~2.0",
        "colee/yii2-vue": "^1.0",
        "dmstr/yii2-adminlte-asset": "^2.4",
        "pheme/yii2-settings": "^0.5.0",
        "johnitvn/yii2-ajaxcrud": "^2.1",
        "kartik-v/yii2-date-range": "^1.6",
        "leandrogehlen/yii2-treegrid": "*",
        "yiidoc/yii2-redactor": "^2.0",
        "php-ai/php-ml": "^0.4.1",
        "yiisoft/yii2-queue": "^2.0",
        "yiisoft/yii2-jui": "^2.0",
