        "paulzi/yii2-json-behavior": "^1.0",
        "paulzi/yii2-file-behavior": "~1.1.0",
        "yii-dream-team/yii2-upload-behavior": "*",
        "pendalf89/yii2-tinymce": "dev-master",
        "pendalf89/yii2-filemanager": "dev-master",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "mongosoft/yii2-upload-behavior": "*",
        "igogo5yo/yii2-upload-from-url": "^1.3",
        "herroffizier/yii2-upload-manager": "@stable",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "paulzi/yii2-nested-sets": "^1.0",
        "jaeger/querylist": "^4.0",
        "yii2mod/yii2-array-query": "*",
        "kriss/yii2-filesystem-qiniu": "^1.0",
        "xutl/yii2-layui-widget": "dev-master",
        "leoshtika/yii2-nav-sidebar": "*",
        "skeeks/yii2-assets-auto-compress": "*",
        "mehrdad-dadkhah/php-fine-uploader": "^1.3",
        "mehrdad-dadkhah/stream-to-download": "^1.0",
        "baibaratsky/yii2-rollbar": "1.5.*",
        "embed/embed": "dev-master",
        "cics/yii2-video-embed-widget": "dev-master",
        "tuyakhov/yii2-youtube": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "mootensai/yii2-enhanced-gii": "^1.0",
        "schmunk42/yii2-giiant": "@stable",
        "warrence/yii2-kartikgii": "dev-master",
        "insolita/yii2-fixturegii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
